{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "oneOf": [
    {
      "description": "price_id is currently the price public_key in Solana. It is available in https://pyth.network/markets/",
      "type": "object",
      "required": [
        "price_info"
      ],
      "properties": {
        "price_info": {
          "type": "object",
          "required": [
            "price_id"
          ],
          "properties": {
            "price_id": {
              "type": "array",
              "items": {
                "type": "integer",
                "format": "uint8",
                "minimum": 0.0
              },
              "maxItems": 32,
              "minItems": 32
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}
